{
  "name": "clips-backend",
  "private": true,
  "description": "Backend for shared clips",
  "version": "0.1.0",
  "scripts": {
    "old-start": "forego start -f Procfile_dev",
    "start": "babel-watch -w src src/server.js",
    "build": "npm run build-server && npm run build-web",
    "build-web": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "build-server": "babel src -d dist",
    "watch": "npm-watch",
    "logs": "heroku logs -t"
  },
  "watch": {
    "build-server": "src/**/*.js"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "6.3.21",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-relay-plugin": "0.6.3",
    "bluebird": "^3.2.2",
    "chroma-js": "^1.1.1",
    "connect-history-api-fallback": "^1.1.0",
    "cors": "^2.7.1",
    "dot": "^1.0.3",
    "express": "4.13.3",
    "is-bot": "0.0.1",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.3.2",
    "radium": "^0.16.6",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-icons": "^1.0.3",
    "react-motion": "^0.4.2",
    "react-progress-label": "^1.2.1",
    "react-relay": "0.6.1",
    "react-router": "^2.0.0-rc4",
    "react-router-relay": "^0.9.0",
    "react-tap-event-plugin": "^0.2.2",
    "npm-watch": "^0.1.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-watch": "^1.0.2"
  }
}
